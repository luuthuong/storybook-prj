{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start",
    "build-package": "turbo run build --filter=app-docs^...",
    "prune": "turbo run prune --scope=app-docs --docker",
    "dev": "turbo run dev --no-cache --continue",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules && rm yarn.lock",
    "publish-package": "turbo run build --filter=docs^... && changeset version && changeset publish"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "@lib/eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@changesets/cli": "^2.26.1"
  }
}
